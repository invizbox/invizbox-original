config wifi-device 'radio0'
	option channel '11'
	option country '00'
	option disabled '0'
	option htmode 'HT20'
	option hwmode '11g'
	option path '10180000.wmac'
	option type 'mac80211'
	option txpower '20'

config wifi-iface 'lan'
	option device 'radio0'
	option encryption 'psk-mixed'
	option ifname 'ap0'
	option isolate '0'
	option key 'TOKENPASSWORD'
	option mode 'ap'
	option network 'lan'
	option ssid 'InvizBox'
